{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "baseUrl": "src",
    "paths": {
      "components/*": [
        "components/*"
      ],
      "utils/*": [
        "utils/*"
      ],
      "types/*": [
        "types/*"
      ],
      "enum/*": [
        "enum/*"
      ],
      "store/*": [
        "store/*"
      ],
      "actions/*": [
        "actions/*"
      ],
      "reducers/*": [
        "reducers/*"
      ],
      "selectors/*": [
        "selectors/*"
      ],
      "saga/*": [
        "saga/*"
      ]
    },
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src",
    "src/custom.d.ts"
  ]
}